<?xml version='1.0' encoding='utf-8'?>
<widget id="mdx.bob.insight" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Insight Proto Test</name>
    <description>
        Insight App.
    </description>
    <author email="b.fields@mdx.ac.uk" href="http://www.cs.mdx.ac.uk">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*://*/*" />
    <access origin="file:///*" />
    <access origin="cdvfile://*" />
    <access origin="content:///*" />
    <allow-navigation href="*" />
    <allow-navigation href="*://*/*" />
    <allow-navigation href="file:///*/*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="file:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="Orientation" value="portrait" />
</widget>
